{
  "dashboardId": "dashboard-id",
  "dataSettings": {
    "dataURL": "https://raw.githubusercontent.com/UNDP-Data/dv-bangladesh/refs/heads/main/public/data/data.csv",
    "keyColumn": "shapeName",
    "fileType": "csv",
    "dataFilters": [
      {
        "column": "shapeName",
        "excludeValues": ["Kawkhali (Betbunia)"]
      }
    ]
  },
  "readableHeader": [
    { "value": "flooded_cropland", "label": "Flooded cropland" },
    { "value": "cropland_not-affected", "label": "Non-affected cropland" }
  ],
  "dashboardLayout": {
    "backgroundColor": false,
    "rows": [
      {
        "columns": [
          {
            "graphType": "donutChart",
            "columnWidth": 1,
            "dataFilters": [
              {
                "column": "indicator",
                "includeValues": ["Flooded cropland", "Non-affected cropland"]
              }
            ],
            "graphDataConfiguration": [
              {
                "columnId": "indicator",

                "chartConfigId": "label"
              },

              {
                "columnId": "value",

                "chartConfigId": "size"
              }
            ],
            "settings": {
              "graphTitle": "Affected Cropland in Bangladesh",
              "padding": "16px 32px 32px 16px",
              "backgroundColor": "#FAFAFA",
              "colors": ["var(--red)", "var(--gray-400)"],
              "graphDownload": true
            }
          }
        ]
      }
    ]
  }
}
